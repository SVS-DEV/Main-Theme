<script>
  // Get the modal
  var modal = document.getElementById('svsModal');
  
  // When the user clicks on <span> (x), close the modal
  function modalclose() {
    modal.style.display = "none";
    setCookie('svspop', 'noshow', 100);
    ga('send', 'event', 'SVS Popup', 'Click', 'Close - X');
}
  
  // When the user clicks anywhere outside of the modal, close it
  window.onclick = function(event) {
    if (event.target == modal) {
      modal.style.display = "none";
      ga('send', 'event', 'SVS Popup', 'Click', 'Close - Clicked Outside Modal');
    }
  }
  
  if (document.cookie.indexOf('svspop=')== -1 && document.location.href.indexOf('system-builder') === -1 && document.location.href.indexOf('test-1') === -1) {
    setTimeout(function(){
      modal.style.display = "block";
      ga('send', 'event', 'SVS Popup', 'Click', 'Popup Shown',{'nonInteraction': 1});
    },20000);
  };
  
  
  var cancel = true;
  function submitpopupform() {
    event.preventDefault();
    if($('#input_4').val() == ''){
      $( "#input_4" ).focus();
      $( "#input_4" ).addClass( "error" );
      $(".emailerror").css("display", "block");
      ga('send', 'event', 'SVS Popup', 'Click', 'Please fill in Email Address');
    }
    else {
      if (cancel == true) { 
        cancel = false;
      jQuery('.modal-top').css("display","none");
      jQuery('.modal-bottom').css("display","none");
      jQuery('.thanks').css("display","block");
      ga('send', 'event', 'SVS Popup', 'Click', 'Submission');
      $( ".jotform-form" ).submit();
      }
    }
  }

  var myVar = setInterval(myTimer, 3000);

function myTimer() {
_ltk.Signup.New("Popup", "input_4", _ltk.Signup.TYPE.CLICK, "input_2");  
_ltk.Signup.SetField("Popup", "input_3");
_ltk.Signup.SetValue("Popup", "contest", "on"); 
};

    
</script>