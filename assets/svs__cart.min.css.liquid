  width: 90%;
  padding: 20px;
  display: block;
  box-sizing: border-box;
  margin: 0 13px;
}

@media screen and (max-width: 639px) {
  .cart th.item,
  .cart td.image,
  .cart td.remove {
    display: none;
  }
  .cart table {
    width: 110%;
    margin-left: -5%;
  }
  .cart .item {
    text-align: left;
  }
  .cart .item a {
    display: block;
    padding: 20px 0;
  }
  .cart .total-box {
    width: 100%;
    float: none;
  }
}
