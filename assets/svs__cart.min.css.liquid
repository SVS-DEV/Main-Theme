}
.cart .product_image img {
  padding-top: 20px;
}
.cart .total-box {
  float: right;
  clear: both;
  background: #eee;
  padding-bottom: 7px;
  width: 400px;
}
.cart .total-box h4 {
  background: #000;
  color: #fff;
  text-transform: uppercase;
  padding: 12px 30px;
  margin-bottom: 12px;
  text-align: center;
}
.cart .total-box > span {
  display: block;
  padding: 0 12px;
  margin-bottom: 5px;
}
.cart .total-box > span span {
  float: right;
