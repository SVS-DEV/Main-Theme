}
.cart .total-box .special-instructions {
  margin-bottom: 13px;
}
.cart .total-box .special-instructions label {
  margin: 13px 0 15px;
  display: block;
  text-align: center;
  padding: 12px 12%;
  background: black;
  color: white;
  font-weight: 700;
  text-transform: uppercase;
}
