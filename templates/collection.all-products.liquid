 <script>
  function selectAll (){
	  	$(".filter").removeClass("active");
	  	document.getElementById('all').className += ' active';
		document.getElementById('1000subwoofer').style.display="block";
        document.getElementById('2000subwoofer').style.display="block";
        document.getElementById('plussubwoofer').style.display="block";
    	document.getElementById('ultrasubwoofer').style.display="block";
        document.getElementById('1000subwooferdual').style.display="block";
        document.getElementById('2000subwooferdual').style.display="block";
        document.getElementById('plussubwooferdual').style.display="block";
        document.getElementById('ultrasubwooferdual').style.display="block";
        document.getElementById('prime-speakers').style.display="block";
        document.getElementById('ultra-speakers').style.display="block";
        document.getElementById('prime-packages').style.display="block";
        document.getElementById('ultra-packages').style.display="block";
        document.getElementById('accessories').style.display="block";
    	ga('send', 'event', 'All Products Collection', 'Click', 'Filtering - All');
 };
 
 function selectSubwoofer (){
	 	  	$(".filter").removeClass("active");
	  	document.getElementById('subwoofer').className += ' active';
	 	document.getElementById('1000subwoofer').style.display="block";
