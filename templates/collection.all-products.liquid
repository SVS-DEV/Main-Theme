{% assign plus_collection = 'plus-series' %}
{% assign 16ultra_collection = '16-ultra-series' %}
{% assign 4000_collection = '4000-series' %}
{% assign prime_collection = 'prime-series' %}
{% assign ultra_collection = 'ultra-series' %}
{% assign svs_accessories = 'accessories' %}
{% assign prime_packages = 'prime-packages' %}
{% assign ultra_packages = 'ultra-systems' %}
{% assign dual_subwoofers_1000 = '1000-series-dual-subwoofers' %}
