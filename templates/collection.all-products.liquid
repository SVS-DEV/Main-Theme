{% include 'landing_page--header', first_heading: collection.metafields.header.first_heading , second_heading: collection.metafields.header.second_heading , description_heading: collection.metafields.header.description_heading, main_description: collection.description %}


  

<section class="collection grid">
 
  <div class="wrap">
    <span class="outletdesc">
    {{ collection.description }}
    </span>
     <nav class="collection__filters">
          	<a href="javascript: void(0)" class="filter active" id="all" onclick="selectAll();">All</a>
            <a href="javascript: void(0)" class="filter" id="subwoofer" onclick="selectSubwoofer();">Subwoofers</a>
            <a href="javascript: void(0)" class="filter" id="dual" onclick="selectDualsubwoofers();">Dual Subwoofers</a>
          	<a href="javascript: void(0)" class="filter" id="speakers" onclick="selectSpeakers();">Speakers</a>
          	<a href="javascript: void(0)" class="filter" id="packages" onclick="selectPackages();">Speaker Systems</a>
          	<a href="javascript: void(0)" class="filter" id="acc" onclick="selectAccessories();">Accessories</a>
    </nav>
  </div>
  <div class="wrap" id="4000subwoofer">
     <h2 class="collection__subheading">New - 4000 Series Subwoofers</h2>

    {% for product in collections[4000_collection].products %}
      {% include 'product-loop-grid' with collection.handle, specs: specs_setting %}
    {% endfor %}
  </div>

  <div class="wrap" id="1000subwoofer">
