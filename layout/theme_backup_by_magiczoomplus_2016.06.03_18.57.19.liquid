  <script>
    function waitForElementToDisplay(selector, time) {
        if(document.querySelector(selector)!=null) {
          $( "#gts-c" ).click(function() {
  ga('send', 'event', 'Google Trusted Stores', 'Click', 'Badge')
            return;
            });
          $( ".gtss-ac" ).click(function() {
  ga('send', 'event', 'Google Trusted Stores', 'Click', 'View Details')
            return;
            });
          $( ".gtss-ub" ).click(function() {
  ga('send', 'event', 'Google Trusted Stores', 'Click', 'Learn More')
            return;
            });
            $("a.gtss-eb:eq(0)").click(function() {
  ga('send', 'event', 'Google Trusted Stores', 'Click', 'Privacy Policy')
            return;
            });
  $("a.gtss-eb:eq(1)").click(function() {
  ga('send', 'event', 'Google Trusted Stores', 'Click', 'Verify')
            return;
            });
  $("a.gtss-eb:eq(2)").click(function() {
  ga('send', 'event', 'Google Trusted Stores', 'Click', 'About Google Trusted Stores')
            return;
            });
        }
        else {
            setTimeout(function() {
                waitForElementToDisplay(selector, time);
            }, time);
        }
    }
    
    waitForElementToDisplay("#gts-c", 2000);
  </script>
