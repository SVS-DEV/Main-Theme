{% assign body_classes="" %}

{% assign cur_page = page_title | handle %}

{% if template contains 'index' %}
  {% assign body_classes="home" %}
{% elsif template == 'product' %}
  {% assign body_classes="product-page" %}
{% elsif template contains 'collection'%}
  {% assign body_classes="collection" %}
  {% if collection.handle contains 'subwoofer' %}
      {% assign body_classes = body_classes | append: ' subwoofers-subpage' %}
  {% endif %}
{% elsif template contains 'series' or template contains 'speakers' or template contains 'subwoofers' or template contains 'upsell' %}
  {% assign body_classes="landing-page " | append: cur_page %}
{% elsif template contains 'systems'%}
  {% assign body_classes="landing-page systems " | append: cur_page %}
{% elsif template contains 'faqs'%}
  {% assign body_classes="faqs updated-basic" %}
{% elsif template contains 'terms-and-conditions'%}
  {% assign body_classes="terms-and-conditions updated-basic" %}
{% elsif template contains 'bill-of-rights'%}
  {% assign body_classes="bill-of-rights updated-basic" %}
{% elsif template contains 'about'%}
  {% assign body_classes="about updated-basic" %}
{% elsif template contains 'privacy-policy'%}
  {% assign body_classes="privacy-policy updated-basic" %}
{% elsif template contains 'document-library'%}
  {% assign body_classes="document-library updated-basic" %}
{% elsif template contains 'customer-service'%}
  {% assign body_classes="customer-service updated-basic" %}
{% elsif template contains 'newsletter'%}
  {% assign body_classes="newsletter updated-basic" %}
{% elsif template contains 'blog' or template contains 'page.reviews' or contains 'featured' %}
  {% assign body_classes="blog" %}
{% elsif template contains 'article'%}
  {% assign body_classes="blog blog-post" %}
{% elsif template contains 'press-reviews'%}
  {% assign body_classes="updated-basic" %}
{% elsif template contains 'us-dealers'%}
  {% assign body_classes="dealers us-dealers" %}
{% elsif template contains 'international-dealers'%}
